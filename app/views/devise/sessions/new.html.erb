<div class="ba">
<div class="fq">
<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="field">
    <%= f.label "メール" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder:"email" %>
  </div>

  <div class="field">
    <%= f.label "パスワード" %><br />
    <%= f.password_field :password, autocomplete: "current-password", placeholder:"半角と英数字6文字以上" %>
  </div>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<div class="q">
<%= render "devise/shared/links" %>
</div>
</div>
</div>
